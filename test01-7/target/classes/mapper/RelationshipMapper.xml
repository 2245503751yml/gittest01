<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dao.RelationshipMapper">
  <resultMap id="BaseResultMap" type="com.example.model.Relationship">
    <result column="c_id" jdbcType="INTEGER" property="cId" />
    <result column="p_id" jdbcType="INTEGER" property="pId" />
  </resultMap>
  
    <sql id="Base_Column_List">
    c_id, p_id
  </sql>
  
  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from relationship
    where c_id = #{cId,jdbcType=INTEGER} and p_id= #{ pId,jdbcType=INTEGER}
  </select>
 
  <insert id="insertSelective" parameterType="com.example.model.Relationship">
    insert into relationship (c_id,p_id)values (#{cId,jdbcType=INTEGER},#{pId,jdbcType=INTEGER})
  
  </insert>
  
  <delete id="deleteByPrimarky" parameterType="com.example.model.Relationship">
  delete from relationship where c_id=#{cId,jdbcType=INTEGER} and p_id= #{ pId,jdbcType=INTEGER}
  </delete>
  
</mapper>